{
  "name": "rpncalc",
  "productName": "rpncalc",
  "version": "0.0.1",
  "description": "Reverse Polish notation calculator",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "watch": "./node_modules/.bin/webpack-dev-server --progress --colors",
    "esdoc": "./node_modules/.bin/esdoc",
    "package-mac": "electron-packager . --overwrite --asar=true --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds",
    "package-linux": "electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds",
    "create-installer-mac": "electron-installer-dmg ./release-builds/rpncalc-darwin-x64/rpncalc.app rpncalc-installer --out=release-builds --overwrite --icon=assets/icons/mac/icon.icns",
    "create-installer-win": "node installers/windows/createinstaller.js"
  },
  "author": "Martin Asser Hansen",
  "email": "mail@maasha.dk",
  "keywords": [
    "calculator",
    "react",
    "electron",
    "rpn",
    "rpncalc"
  ],
  "license": "GPL-2.0",
  "repository": "https://github.com/maasha/rpncalc",
  "dependencies": {
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babelify": "^7.2.0",
    "classnames": "^2.2.3",
    "node-sass": "^4.5.2",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-tooltip": "^3.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.24.0",
    "css-loader": "^0.28.0",
    "electron": "^1.6.2",
    "electron-installer-dmg": "^0.2.1",
    "electron-packager": "^8.6.0",
    "electron-react-devtools": "^0.4.0",
    "electron-winstaller": "^2.5.2",
    "esdoc": "^0.5.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "ink-docstrap": "^1.3.0",
    "jsdoc": "^3.4.3",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.3.3",
    "webpack-dev-server": "^2.4.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
