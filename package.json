{
  "name": "rpncalc",
  "version": "0.0.1",
  "description": "Reverse Polish notation calculator",
  "main": "main.js",
  "scripts": {
    "start": "electron main.js",
    "watch": "watchify app/app.js -t babelify -o public/js/bundle.js --debug --verbose",
    "package-mac": "electron-packager . --overwrite --asar=true --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=release-builds",
    "package-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds",
    "package-linux": "electron-packager . --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png --prune=true --out=release-builds",
    "create-installer-mac": "electron-installer-dmg ./release-builds/rpncalc-app-darwin-x64/rpncalc app.app electron-tutorial-app --out=release-builds --overwrite --icon=assets/icons/mac/icon.icns",
    "create-installer-win": "node installers/windows/createinstaller.js"
  },
  "author": "Martin Asser Hansen",
  "email": "mail@maasha.dk",
  "license": "GPL-2.0",
  "repository": "https://github.com/maasha/rpncalc",
  "dependencies": {
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babelify": "^7.2.0",
    "classnames": "^2.2.3",
    "electron-prebuilt": "^1.4.13",
    "electron-reload": "^1.1.0",
    "react": "^15.4.2",
    "react-burger-menu": "^1.10.14",
    "react-dom": "^15.4.2",
    "react-modal": "^1.7.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.24.0",
    "electron": "^1.6.2",
    "electron-react-devtools": "^0.4.0",
    "electron-winstaller": "^2.5.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
